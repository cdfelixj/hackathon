# Environment variables for Maps Area Insights Frontend

# Google Maps API
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Backend API
REACT_APP_BACKEND_URL=http://localhost:3001
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Firebase Configuration (Client-side)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Application Settings
REACT_APP_DEFAULT_MAP_CENTER_LAT=40.7128
REACT_APP_DEFAULT_MAP_CENTER_LNG=-74.0060
REACT_APP_DEFAULT_MAP_ZOOM=12

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_OFFLINE_MODE=false
REACT_APP_DEBUG_MODE=false

# Cache Settings
REACT_APP_CACHE_DURATION_MINUTES=60
REACT_APP_MAX_CACHED_SEARCHES=100
