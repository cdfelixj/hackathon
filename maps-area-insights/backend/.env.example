# Environment variables for Maps Area Insights Backend

# Server Configuration
PORT=3001
NODE_ENV=development

# Google APIs
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firebase Configuration
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_firebase_private_key_here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your_firebase_client_email@your_project.iam.gserviceaccount.com

# API Configuration
API_RATE_LIMIT_WINDOW_MS=900000
API_RATE_LIMIT_MAX_REQUESTS=100
PLACES_SEARCH_RADIUS=3000
MAX_PLACES_RESULTS=20

# Cache Configuration
CACHE_TTL_HOURS=24
POPULAR_AREAS_CACHE_TTL_HOURS=168

# Security
CORS_ORIGIN=http://localhost:3000
